topology "standalone"

<% unless @chef_analytics['api_fqdn'].nil? || @chef_analytics['api_fqdn'].empty? -%>
analytics_fqdn "<%= @chef_analytics['api_fqdn'] %>"
<% end -%>

<% @chef_analytics['configuration'].each_pair do |component, tunables| -%>
<% case tunables -%>
<% when Hash -%>
<% tunables.each_pair do |name, value| -%>
<% value = %Q["#{value}"] if value.kind_of?(String) -%>
<%= "#{component.gsub("-","_")}['#{name}'] = #{value}" %>
<% end -%>
<% when String -%>
<%= component.gsub('-', '_') %> "<%= tunables %>"
<% end -%>
<% end -%>